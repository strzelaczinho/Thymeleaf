<thlogic>
<attr sel="#headId"         th:replace="~{fragments/head :: headFragment}" />
<attr sel="#game-title"     th:text="#{game.title}"/>
<attr sel="#main-message"   th:text="${__${T(adam.projekty.util.AttributeNames).MAIN_MESSAGE}__}"/>
<attr sel="#result-message" th:text="${__${T(adam.projekty.util.AttributeNames).RESULT_MESSAGE}__}"/>
<attr sel="#game-form"      th:action="@{__${T(adam.projekty.util.GameMappings).PLAY}__}" action="play" method="post"/>
<attr sel="#enter-guess"    th:text="#{game.enter.guess}"/>
<attr sel="#submit"         th:value="#{game.submit}"/>
<attr sel="#footerId"       th:replace="~{fragments/footer :: footer}"/>

</thlogic>